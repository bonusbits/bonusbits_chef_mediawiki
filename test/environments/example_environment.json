{
    "name": "example_environment",
    "description": "Environment",
    "cookbook_versions": {},
    "json_class": "Chef::Environment",
    "chef_type": "environment",
    "default_attributes": {
    },
    "override_attributes": {
        "bonusbits_mediawiki_nginx": {
            "aws": {
                "stack_name": "mediawiki-nginx-kitchen-01",
                "vpc_id": "vpc-00000000",
                "create_logs_group": true,
                "logs_group_name": "mediawiki-nginx-kitchen",
                "efs_filesystem_id": "fs-00000000",
                "s3_deploy_path": "example-deploy/mediawiki"
            },
            "mediawiki": {
                "version_major": "1",
                "version_minor": "28",
                "site_folder_name": "mediawiki",
                "uploads_folder_name": "uploads"
            },
            "nginx": {
                "root_site_path": "/var/www/html",
                "x_forwarded_traffic": false,
                "rewrite_wiki_alias": false
            },
            "dns": {
                "configure": true
            },
            "adsense": {
                "configure": true
            },
            "backups": {
                "configure": true,
                "backup_interval": "hourly"
            }
        }
    }
}